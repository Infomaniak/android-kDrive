<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/file_shared_navigation"
    app:startDestination="@id/fileSharedListFragment">

    <activity
        android:id="@+id/fileSharedActivity"
        android:name="com.infomaniak.drive.ui.fileShared.FileSharedActivity"
        tools:layout="@layout/activity_file_shared"/>

    <fragment
        android:id="@+id/fileSharedPasswordFragment"
        android:name="com.infomaniak.drive.ui.fileShared.FileSharedPasswordFragment"
        tools:layout="@layout/fragment_file_shared_password" />

    <fragment
        android:id="@+id/fileSharedListFragment"
        android:name="com.infomaniak.drive.ui.fileShared.FileSharedListFragment"
        tools:layout="@layout/fragment_file_shared_list">

        <argument
            android:name="fileUri"
            app:argType="android.net.Uri"
            app:nullable="true"
            android:defaultValue="@null"/>

    </fragment>

</navigation>
